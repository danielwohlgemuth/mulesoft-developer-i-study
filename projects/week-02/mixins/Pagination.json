{
    "openapi": "3.0.0",
    "info": {
      "version": "1.0.0",
      "title": "Pagination"
    },
    "components": {
      "schemas": {
        "Page": {
          "name": "page",
          "in": "query",
          "required": false,
          "description": "Specifies the page of results to return",
          "schema": {
            "type": "integer",
            "default": 1,
            "minimum": 1,
            "maximum": 100,
            "example": 2
          }
        },
        "Limit": {
          "name": "limit",
          "in": "query",
          "required": false,
          "description": "Specifies how many records are returned",
          "schema": {
            "type": "integer",
            "default": 10,
            "minimum": 1,
            "maximum": 100,
            "example": 20
          }
        }
      }
    },
    "paths": {}
  }